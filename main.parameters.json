{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "westeurope"
    },
    "acrName": {
      "value": "myacrname"
    },
    "appServicePlanName": {
      "value": "myappserviceplan"
    },
    "webAppName": {
      "value": "mywebappname"
    },
    "containerRegistryImageName": {
      "value": "myflaskapp"
    },
    "containerRegistryImageVersion": {
      "value": "latest"
    },
    "keyVaultName": {
      "value": "mykeyvault-name"
    },
    "tenantId": {
      "value": "your-tenant-id"
    },
    "objectId": {
      "value": "your-service-principal-object-id"
    }
  }
}
